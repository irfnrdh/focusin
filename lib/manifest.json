{
  "manifest_version": 3,
  "name": "Focusin Pomodoro",
  "version": "1.0",
  "description": "A Chrome extension for pomodoro and reverse pomodoro.",
  "permissions": [
    "alarms",
    "tabs",
    "activeTab",
    "scripting",
    "storage",
    "notifications",
    "webNavigation",
    "downloads",
    "history"
  ],  
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}